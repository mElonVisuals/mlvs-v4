    <section class="features-section" id="commands">
      <div class="container">
        <div class="sec-head">
          <div class="section-badge glass-morphism">
            <svg class="nav-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <polygon points="13,2 3,14 12,14 11,22 21,10 12,10 13,2"/>
            </svg>
            Commands Reference
          </div>
          <h2 class="sec-title">
            Available <span class="highlight-text gradient-text">Commands</span>
          </h2>
          <p class="sec-intro">
            Complete list of all bot commands organized by category.
          </p>
        </div>
        
        <div class="features-grid">
          <% if (commands && Object.keys(commands).length > 0) { %>
            <% Object.keys(commands).forEach(category => { %>
              <div class="feature-card glass-morphism">
                <div class="feature-header">
                  <div class="feature-icon gradient-bg">
                    <% if (category === 'general') { %>
                      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <path d="M12 20h9"/>
                        <path d="M16.5 3.5a2.121 2.121 0 0 1 3 3L7 19l-4 1 1-4L16.5 3.5z"/>
                      </svg>
                    <% } else if (category === 'moderation') { %>
                      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <rect x="3" y="11" width="18" height="11" rx="2" ry="2"/>
                        <circle cx="12" cy="16" r="1"/>
                        <path d="M7 11V7a5 5 0 0 1 10 0v4"/>
                      </svg>
                    <% } else if (category === 'fun') { %>
                      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <circle cx="12" cy="12" r="10"/>
                        <path d="M8 14s1.5 2 4 2 4-2 4-2"/>
                        <line x1="9" y1="9" x2="9.01" y2="9"/>
                        <line x1="15" y1="9" x2="15.01" y2="9"/>
                      </svg>
                    <% } else if (category === 'music') { %>
                      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <path d="M9 18V5l12-2v13"/>
                        <circle cx="6" cy="18" r="3"/>
                        <circle cx="18" cy="16" r="3"/>
                      </svg>
                    <% } else if (category === 'admin') { %>
                      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                      </svg>
                    <% } else { %>
                      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z"/>
                        <polyline points="14,2 14,8 20,8"/>
                        <line x1="16" y1="13" x2="8" y2="13"/>
                        <line x1="16" y1="17" x2="8" y2="17"/>
                        <polyline points="10,9 9,9 8,9"/>
                      </svg>
                    <% } %>
                  </div>
                  <div class="feature-badge"><%= commands[category].length %> commands</div>
                </div>
                <div class="feature-content">
                  <h3 class="feature-title"><%= category.charAt(0).toUpperCase() + category.slice(1) %></h3>
                  <p class="feature-desc">
                    <% if (commands[category].length > 0) { %>
                      <strong>!<%= commands[category][0].name %></strong><% if (commands[category].length > 1) { %>, <strong>!<%= commands[category][1].name %></strong><% } %><% if (commands[category].length > 2) { %>, and <%= commands[category].length - 2 %> more<% } %>
                    <% } %>
                  </p>
                </div>
                <div class="feature-footer">
                  <div class="feature-metrics">
                    <span class="metric">Category: <%= category %></span>
                    <span class="metric-sep">â€¢</span>
                    <span class="metric"><%= commands[category].length %> total</span>
                  </div>
                </div>
              </div>
            <% }) %>
          <% } else { %>
            <div class="feature-card glass-morphism">
              <div class="feature-header">
                <div class="feature-icon gradient-bg">
                  <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                    <path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z"/>
                    <polyline points="14,2 14,8 20,8"/>
                    <line x1="16" y1="13" x2="8" y2="13"/>
                    <line x1="16" y1="17" x2="8" y2="17"/>
                    <polyline points="10,9 9,9 8,9"/>
                  </svg>
                </div>
                <div class="feature-badge">Coming Soon</div>
              </div>
              <div class="feature-content">
                <h3 class="feature-title">Commands Loading</h3>
                <p class="feature-desc">
                  Commands are being loaded. Please check back in a moment.
                </p>
              </div>
            </div>
          <% } %>
        </div>
      </div>
    </section>

